    },
    "AddFactor": {
      "Type": "Task",
      "Resource": "function:riskModel_addRiskFactor",
      "End": true
    }
  },
  "restrictions": [
    {
      "roleId": "$authenticated",
      "allows": [
        "*"
      ]
    }
  ]
